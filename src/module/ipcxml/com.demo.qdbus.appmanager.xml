<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
        "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="com.demo.qdbus.AppManager">
        <method name="Query">
            <arg name="appID" type="s" direction="in"/>
            <arg name="paramMap" type="{ss}" direction="in"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.In1" value="ParamStringMap"/>
            <arg name="AppInfoList" type="a(s)" direction="out"/>
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="AppInfoList"/>
        </method>
    </interface>
</node>